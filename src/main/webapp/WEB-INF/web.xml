<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>WebProjectTomcat</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file> 
  </welcome-file-list>
  <servlet>
        <servlet-name>PrimerServlet</servlet-name>
        <servlet-class>controlador.PrimerServlet</servlet-class>
    </servlet>
  <servlet>
        <servlet-name>SegundoServlet</servlet-name>
        <servlet-class>controlador.SegundoServlet</servlet-class>
    </servlet> 
    
    <servlet-mapping>
    	<servlet-name>SegundoServlet</servlet-name>
    	<url-pattern>/SegundoServlet</url-pattern>
    </servlet-mapping>
</web-app>